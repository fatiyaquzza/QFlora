{
  "expo": {
    "name": "QFlora",
    "slug": "QFlora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/splash-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.usk.qflora",
      "googleServicesFile": "./android/app/google-services.json",
      "sha1": "5E:8F:16:06:2E:A3:CD:2C:4A:0D:54:78:76:BA:A6:F3:8C:AB:F6:25"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "package": "com.usk.qflora",
      "favicon": "./assets/icons/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon.png",
          "imageWidth": 200,
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "353e16cc-9759-4567-b351-71e57450d870"
      }
    }
  }
}